[
    {
        "@type": "query",
        "name": "Recently Added",
        "sort": "-inCollectionSince"
    },
    {
        "@type": "query",
        "name": "Recently Modified",
        "sort": "-lastRevisionedAt"
    },
    {
        "@type": "query",
        "name": "Compilations",
        "sort": "+albumTitle",
        "filter": {
            "phrase": [
                [
                    "albumArtist"
                ],
                [
                    "various",
                    "artists"
                ]
            ]
        }
    },
    {
        "@type": "group",
        "name": "1st Level",
        "desc": "A group of items on the 1st level",
        "items": [
            {
                "@type": "group",
                "name": "2nd Level",
                "desc": "A nested group of items on the 2nd level",
                "items": [
                    {
                        "@type": "query",
                        "name": "Top 40",
                        "sort": "+musicTempo,+trackArtist",
                        "filter": {
                            "tag": {
                                "label": {
                                    "matches": "top40"
                                }
                            }
                        }
                    }
                ]
            }
        ]
    },
    {
        "@type": "group",
        "name": "Epoch",
        "items": [
            {
                "@type": "query",
                "name": "1960s",
                "sort": "+musicTempo,+trackArtist",
                "filter": {
                    "tag": {
