    {
        "@type": "group",
        "name": "Tempo",
        "items": [
            {
                "@type": "query",
                "name": "<80 bpm",
                "sort": "+musicTempo,+trackArtist",
                "filter": {
                    "all": [
                        {
                            "numeric": [
                                "musicTempo",
                                {
                                    "gt": 0
                                }
                            ]
                        },
                        {
                            "numeric": [
                                "musicTempo",
                                {
                                    "lt": 80
                                }
                            ]
                        }
                    ]
                }
            },
            {
                "@type": "query",
                "name": "80-100 bpm",
                "sort": "+musicTempo,+trackArtist",
                "filter": {
                    "all": [
                        {
                            "numeric": [
                                "musicTempo",
                                {
                                    "ge": 80
                                }
                            ]
                        },
                        {
                            "numeric": [
                                "musicTempo",
                                {
                                    "le": 100
                                }
                            ]
                        }
                    ]
                }
            },
