    {
        "@type": "group",
        "name": "Selection",
        "items": [
            {
                "@type": "query",
                "name": "DJ",
                "desc": "Selected tracks for DJing with known BPM (sorted)",
                "sort": "+musicTempo,+trackArtist",
                "filter": {
                    "all": [
                        {
                            "tag": {
                                "facets": [
                                    "selection"
                                ],
                                "label": {
                                    "matches": "dj"
                                }
                            }
                        },
                        {
                            "numeric": [
                                "musicTempo",
                                {
                                    "gt": 0
                                }
                            ]
                        }
                    ]
                }
            }
        ]
    },
    {
        "@type": "group",
        "name": "Library",
        "items": [
            {
                "@type": "query",
                "name": "Missing album artist or title",
                "desc": "Find all tracks with an album title but no album artist or vice versa",
                "sort": "+albumArtist,+albumTitle,+trackNumber",
                "filter": {
                    "any": [
                        {
                            "all": [
                                {
                                    "not": {
                                        "phrase": [
                                            [
                                                "albumTitle"
                                            ],
                                            []
                                        ]
                                    }
                                },
                                {
                                    "phrase": [
                                        [
                                            "albumArtist"
                                        ],
                                        []
                                    ]
                                }
                            ]
                        },
                        {
                            "all": [
                                {
                                    "not": {
                                        "phrase": [
                                            [
                                                "albumArtist"
                                            ],
                                            []
                                        ]
                                    }
                                },
                                {
                                    "phrase": [
                                        [
                                            "albumTitle"
                                        ],
                                        []
                                    ]
                                }
                            ]
                        }
                    ]
                }
            },
            {
                "@type": "query",
                "name": "Missing track number",
                "desc": "Find all album tracks with missing track number",
                "sort": "+albumArtist,+albumTitle",
                "filter": {
                    "all": [
                        {
                            "not": {
                                "phrase": [
                                    [
                                        "albumTitle"
                                    ],
                                    []
                                ]
                            }
                        },
                        {
                            "not": {
                                "numeric": [
                                    "trackNumber",
                                    {
                                        "gt": 0
                                    }
                                ]
                            }
                        }
                    ]
                }
            }
        ]
    }
]